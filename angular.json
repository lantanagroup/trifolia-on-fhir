{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "client": {
      "root": "apps/client/",
      "sourceRoot": "apps/client/src",
      "projectType": "application",
      "prefix": "trifolia-fhir",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/client",
            "index": "apps/client/src/index.html",
            "main": "apps/client/src/main.ts",
            "polyfills": "apps/client/src/polyfills.ts",
            "tsConfig": "apps/client/tsconfig.app.json",
            "assets": [
              "apps/client/src/favicon.ico",
              "apps/client/src/assets",
              "apps/client/src/help",
              "apps/client/src/silent-refresh.html",
              {
                "glob": "**/*",
                "input": "libs/tof-lib/src/assets",
                "output": "./assets"
              }
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "node_modules/ngx-simplemde/src/index.less",
              "apps/client/src/styles.css"
            ],
            "scripts": [
              "node_modules/@popperjs/core/dist/umd/popper.min.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/simplemde-antd/dist/simplemde.min.js"
            ],
            "aot": false,
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/client/src/environments/environment.ts",
                  "with": "apps/client/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "8mb",
                  "maximumError": "11mb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"],
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "client:build",
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "client:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "client:build"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/client/jest.config.ts",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/client/"]
        }
      },
      "tags": []
    },
    "client-e2e": {
      "root": "apps/client-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/client-e2e/protractor.conf.js",
            "devServerTarget": "client:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "client:serve:production"
            }
          }
        }
      },
      "tags": []
    },
    "server": {
      "root": "apps/server",
      "sourceRoot": "apps/server/src",
      "projectType": "application",
      "prefix": "server",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/webpack:webpack",
          "options": {
            "outputPath": "dist/apps/server",
            "main": "apps/server/src/main.ts",
            "tsConfig": "apps/server/tsconfig.app.json",
            "assets": [
              "apps/server/src/assets",
              "apps/server/src/config",
              "apps/server/src/igs",
              {
                "glob": "**/*",
                "input": "libs/tof-lib/src/assets",
                "output": "./assets"
              }
            ],
            "buildLibsFromSource": true,
            "target": "node",
            "compiler": "tsc"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/server/src/environments/environment.ts",
                  "with": "apps/server/src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@nrwl/node:node",
          "options": {
            "buildTarget": "server:build"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/server/jest.config.ts",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/server"]
        }
      },
      "tags": []
    },
    "tof-lib": {
      "root": "libs/tof-lib",
      "sourceRoot": "libs/tof-lib/src",
      "projectType": "library",
      "prefix": "trifolia-fhir",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/tof-lib/jest.config.ts",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/tof-lib"]
        }
      },
      "schematics": {},
      "tags": []
    },
    "tools": {
      "root": "apps/tools",
      "sourceRoot": "apps/tools/src",
      "projectType": "application",
      "prefix": "tools",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/webpack:webpack",
          "options": {
            "outputPath": "dist/apps/tools",
            "main": "apps/tools/src/app/main.ts",
            "tsConfig": "apps/tools/tsconfig.app.json",
            "assets": ["apps/tools/src/assets"],
            "buildLibsFromSource": true,
            "target": "node",
            "compiler": "tsc"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@nrwl/node:node",
          "options": {
            "buildTarget": "tools:build"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/tools/jest.config.ts",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/tools"]
        }
      },
      "tags": []
    }
  }
}
